{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "no-console": "off",
    "no-fallthrough": "off",
    "react/no-string-refs": "off",
    "react/jsx-key": "off"
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
